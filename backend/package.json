{
  "name": "gws-email-backup-backend",
  "version": "1.0.0",
  "description": "Google Workspace Email Backup Backend",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:highmem": "node --max-old-space-size=4096 src/index.js",
    "dev": "nodemon src/index.js",
    "dev:highmem": "nodemon --max-old-space-size=4096 src/index.js",
    "dev:windows": "set NODE_ENV=development&& nodemon src/index.js",
    "test": "jest",
    "setup:windows": "npm run setup:db && npm run setup:redis",
    "setup:db": "node scripts/setup-db.js",
    "setup:admin": "node scripts/setup-admin.js",
    "setup:redis": "echo Redis setup instructions in README.md",
    "clean": "rimraf logs backup temp exports"
  },
  "dependencies": {
    "archiver": "^6.0.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "google-auth-library": "^9.4.1",
    "helmet": "^7.1.0",
    "imap": "^0.8.17",
    "imapflow": "^1.2.6",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.6.5",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "rimraf": "^5.0.5",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
